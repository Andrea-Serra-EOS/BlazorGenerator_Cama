@inherits BlazorGeneratorComponentBase

@if (NavManager.ToBaseRelativePath(NavManager.Uri) != "")
{
  <FluentButton IconStart="new Icons.Regular.Size20.ArrowCircleLeft()"
                OnClick="@(() => JSRuntime.InvokeVoidAsync("history.back"))" Title="Go Back (Alt + ←)" Style=" margin-left: -15px; width:100px;">
    @Captions.Instance.NavigationBack
  </FluentButton>
}