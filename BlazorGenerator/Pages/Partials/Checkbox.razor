@typeparam T


<Check TValue="bool"
	   Checked="@((bool)Field.Getter(Data))"
	   ReadOnly="@(!Field.Editable)"
	   @onchange="@((v)=> Field.Setter(Data, v.Value))"
       CheckedChanged="@((v) => {Field.Setter(Data,v);})" Disabled="@(!Field.Editable)" />

@code {
	[Parameter]
	public VisibleField<T> Field { get; set; }

	[Parameter]
	public T Data { get; set; }
}